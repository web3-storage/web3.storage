{
  "name": "@web3-storage/website",
  "version": "2.39.4",
  "description": "web3.storage website",
  "private": true,
  "scripts": {
    "ci:envfile": "ts-node -O '{\"module\": \"commonjs\"}' scripts/envfile.ts ",
    "dev": "rm -rf .next && next dev -p 4000",
    "did-document": "ts-node -O '{\"module\": \"commonjs\"}' scripts/did-document.ts",
    "start": "env-cmd -f ../../.env next dev -p 4000",
    "prebuild": "sh ./scripts/prebuild.sh",
    "build": "next build && npm run build:did.json && next-sitemap && next export",
    "build:did.json": "npm run -s did-document > public/.well-known/did.json",
    "postbuild": "sh ./scripts/postbuild.sh",
    "export": "next export",
    "test": "eslint './**/*.js' && tsc --build",
    "test:e2e": "env-cmd -f ../../.env npx playwright test",
    "lint": "npm run lint:ts && npm run lint:es",
    "lint:fix": "npm run lint:es:fix",
    "lint:ts": "tsc --build",
    "lint:es": "eslint --max-warnings=0",
    "lint:es:fix": "npm run lint:es -- --fix",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 9000",
    "build-storybook": "build-storybook",
    "prepare": "cd ../.. && husky install packages/website/.husky"
  },
  "dependencies": {
    "@docsearch/react": "^3.0.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@magic-ext/oauth": "^0.7.0",
    "@mdx-js/react": "^2.1.1",
    "@stripe/react-stripe-js": "^1.10.0",
    "@stripe/stripe-js": "^1.35.0",
    "@web3-storage/parse-link-header": "^3.1.0",
    "@web3-storage/w3up-launch": "^1.0.0",
    "algoliasearch": "^4.13.0",
    "clsx": "^1.1.1",
    "countly-sdk-web": "^20.11.2",
    "example-image-gallery": "github:web3-storage/example-image-gallery#main",
    "filesize": "^6.1.0",
    "highlight.js": "11.4.0",
    "kwesforms": "^2.1.22",
    "magic-sdk": "^4.2.1",
    "next": "^12.1.0",
    "next-optimized-images": "^2.6.2",
    "nextra": "^2.0.0-beta.5",
    "p-map": "^5.1.0",
    "pretty-bytes": "^5.6.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.4",
    "react-icons": "^4.4.0",
    "react-instantsearch-dom": "^6.23.4",
    "react-markdown": "^6.0.3",
    "react-redux": "7.2.6",
    "react-syntax-highlighter": "^15.4.5",
    "redux": "4.1.2",
    "remark-slug": "6.1.0",
    "scrollmagic": "2.0.8",
    "scrollmagic-plugins": "1.0.8",
    "swagger-ui-dist": "^4.10.3",
    "web3.storage": "^3.5.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.0.8",
    "@playwright/test": "^1.28.1",
    "@svgr/webpack": "^6.2.1",
    "@types/react": "17.0.36",
    "@types/react-dom": "17.0.11",
    "@types/react-syntax-highlighter": "13.5.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-eslint": "10.1.0",
    "css-loader": "5.2.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "git-rev-sync": "^3.0.2",
    "git-revision-webpack-plugin": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.2.4",
    "next-sitemap": "1.6.173",
    "node-fetch-commonjs": "3.2.4",
    "prettier": "2.4.1",
    "react-query": "3.33.5",
    "sass": "1.43.4",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.0",
    "url-loader": "4.1.1"
  },
  "lint-staged": {
    "{pages,components,hooks,content,styles,store}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "{pages,components,hooks,content,store}/**/!(types.tsx).{js,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
