{
  "name": "@web3-storage/api",
  "version": "7.25.0",
  "private": true,
  "type": "module",
  "license": "(Apache-2.0 OR MIT)",
  "main": "./dist/worker.js",
  "scripts": {
    "lt": "npm-run-all -p lt:*",
    "lt:cluster": "npx localtunnel --port 9094 --subdomain \"$(whoami)-cluster-api-web3-storage\"",
    "lt:postgres": "npx localtunnel --port 3000 --subdomain \"$(whoami)-postgres-api-web3-storage\"",
    "start": "npm-run-all -s minio:start -p  \"minio:mkbucket -- dotstorage-dev-0\" cluster:start pg:start -s miniflare",
    "start:preview": "npm-run-all -s minio:start -p \"minio:mkbucket -- dotstorage-dev-0\" cluster:start pg:start -s -p lt wrangler-dev",
    "stop": "npm-run-all -p minio:stop cluster:stop pg:stop",
    "wrangler-dev": "wrangler dev --env $(whoami)",
    "publish": "wrangler publish --env $(whoami)",
    "build": "run-p build:cli typecheck",
    "build:cli": "node scripts/cli.js build",
    "test": "run-s test:worker test:size",
    "test:worker": "mocha --require test/hooks.js --experimental-vm-modules",
    "test:size": "npx bundlesize",
    "typecheck": "tsc -b ./tsconfig.json",
    "cluster:start": "web3-storage-tools cluster --start",
    "cluster:stop": "web3-storage-tools cluster --stop",
    "cluster:reset": "web3-storage-tools cluster --clean",
    "minio:start": "web3-storage-tools minio server start",
    "minio:mkbucket": "web3-storage-tools minio bucket create",
    "minio:stop": "web3-storage-tools minio server stop",
    "minio:reset": "web3-storage-tools minio server clean",
    "pg:start": "cd ../db && npm start",
    "pg:stop": "cd ../db && npm run stop",
    "miniflare": "miniflare dist/worker.js --modules --env ../../.env --r2 CARPARK --r2 DUDEWHERE --r2 SATNAV $API_MINIFLARE_FLAGS --watch",
    "lint": "standard --verbose",
    "lint:fix": "npm run lint -- --fix",
    "stripe.com:fixtures": "stripe fixtures src/stripe.com/fixtures.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@miniflare/core": "^2.8.1",
    "@sentry/cli": "^1.72.1",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.13",
    "@ucanto/interface": "^8.0.0",
    "@web-std/fetch": "^3.0.2",
    "@web-std/form-data": "^3.0.0",
    "@web3-storage/tools": "^1.0.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.14",
    "git-rev-sync": "^3.0.1",
    "miniflare": "^2.14.0",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "p-defer": "^4.0.0",
    "process": "^0.11.10",
    "sade": "^1.8.1",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "websocket": "^1.0.34"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.53.1",
    "@cfworker/json-schema": "^1.8.3",
    "@google-cloud/precise-date": "^2.0.4",
    "@ipld/car": "^5.2.0",
    "@ipld/dag-pb": "^4.0.4",
    "@magic-ext/oauth": "^0.8.0",
    "@magic-sdk/admin": "^1.3.0",
    "@nftstorage/ipfs-cluster": "^5.0.1",
    "@ucanto/core": "^8.0.0",
    "@ucanto/principal": "^8.0.0",
    "@web3-storage/car-block-validator": "^1.0.0",
    "@web3-storage/content-claims": "^3.0.1",
    "@web3-storage/db": "^4.1.0",
    "@web3-storage/w3up-launch": "^1.0.0",
    "@web3-storage/multipart-parser": "^1.0.0",
    "cardex": "^2.3.1",
    "carstream": "^1.1.0",
    "cborg": "^1.6.0",
    "ipfs-car": "^0.7.0",
    "itty-router": "^2.4.8",
    "linkdex": "^3.0.0",
    "lnmap": "^1.0.1",
    "lnset": "^1.2.0",
    "multiformats": "^12.0.1",
    "nanoid": "^3.1.30",
    "p-retry": "^5.1.1",
    "protobufjs": "^6.11.2",
    "sinon": "^14.0.2",
    "stripe": "^10.7.0",
    "toucan-js": "^2.7.0",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^3.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/worker.js",
      "maxSize": "1 MB"
    }
  ]
}
